<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp
    xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0"
    xmlns:mailappor="http://schemas.microsoft.com/office/mailappversionoverrides/1.0" xsi:type="MailApp">
    <Id>bf119e1f-c933-47ee-93bf-d4864c4db418</Id>
    <Version>1.0.0.0</Version>
    <ProviderName>MeetSa</ProviderName>
    <DefaultLocale>en-US</DefaultLocale>
    <DisplayName DefaultValue="Meet.sa"/>
    <Description DefaultValue="Meet anyone with a click of a button."/>
    <IconUrl DefaultValue="https://meet-outlook-addin.cloud.site.sa/icon-32.png"/>
    <HighResolutionIconUrl DefaultValue="https://meet-outlook-addin.cloud.site.sa/icon-128.png"/>
    <SupportUrl DefaultValue="https://www.meet.sa/help"/>
    <AppDomains>
        <AppDomain>meet.sa</AppDomain>
    </AppDomains>
    <Hosts>
        <Host Name="Mailbox"/>
    </Hosts>
    <Requirements>
        <Sets>
            <Set Name="Mailbox" MinVersion="1.1"/>
        </Sets>
    </Requirements>
    <FormSettings>
        <Form xsi:type="ItemRead">
            <DesktopSettings>
                <SourceLocation DefaultValue="https://meet-outlook-addin.cloud.site.sa/commands.html"/>
                <RequestedHeight>250</RequestedHeight>
            </DesktopSettings>
        </Form>
    </FormSettings>
    <Permissions>ReadWriteItem</Permissions>
    <Rule xsi:type="RuleCollection" Mode="Or">
        <Rule xsi:type="ItemIs" ItemType="Message" FormType="Read"/></Rule>
    <DisableEntityHighlighting>false</DisableEntityHighlighting>
    <VersionOverrides
        xmlns="http://schemas.microsoft.com/office/mailappversionoverrides" xsi:type="VersionOverridesV1_0">
        <Requirements>
            <bt:Sets DefaultMinVersion="1.3">
                <bt:Set Name="Mailbox"/>
            </bt:Sets>
        </Requirements>
        <Hosts>
            <Host xsi:type="MailHost">
                <DesktopFormFactor>
                    <FunctionFile resid="Commands.Url"/>
                    <ExtensionPoint xsi:type="MessageReadCommandSurface">
                        <OfficeTab id="TabDefault">
                            <Group id="msgComposeCmdGroup">
                                <Label resid="Meet.sa"/>
                                <Control xsi:type="Menu" id="msgComposeInsertDefaultGist">
                                    <Label resid="ScheduleMeeting"/>
                                    <Supertip>
                                        <Title resid="ScheduleMeeting"/>
                                        <Description resid="ScheduleMeeting"/>
                                    </Supertip>
                                    <Icon>
                                        <bt:Image size="16" resid="Icon.16x16"/>
                                        <bt:Image size="32" resid="Icon.32x32"/>
                                        <bt:Image size="80" resid="Icon.80x80"/>
                                    </Icon>
                                    <Items>
                                        <Item id="site.meet.sa">
                                            <Label resid="site.meet.sa.label"/>
                                            <Supertip>
                                                <Title resid="site.meet.sa.label" />
                                                <Description resid="site.meet.sa.label" />
                                            </Supertip>
                                            <Action xsi:type="ExecuteFunction">
                                                <FunctionName>genUrl</FunctionName>
                                            </Action>
                                        </Item>
                                        <Item id="site-internal.meet.sa">
                                            <Label resid="site-internal.meet.sa.label"/>
                                            <Supertip>
                                                <Title resid="site-internal.meet.sa.label" />
                                                <Description resid="site-internal.meet.sa.label" />
                                            </Supertip>
                                            <Action xsi:type="ExecuteFunction">
                                                <FunctionName>genUrl</FunctionName>
                                            </Action>
                                        </Item>
                                    </Items>
                                </Control>
                            </Group>
                        </OfficeTab>
                    </ExtensionPoint>
                </DesktopFormFactor>
            </Host>
        </Hosts>
        <Resources>
            <bt:Images>
                <bt:Image id="Icon.16x16" DefaultValue="https://meet-outlook-addin.cloud.site.sa/icon-16.png"/>
                <bt:Image id="Icon.32x32" DefaultValue="https://meet-outlook-addin.cloud.site.sa/icon-32.png"/>
                <bt:Image id="Icon.80x80" DefaultValue="https://meet-outlook-addin.cloud.site.sa/icon-80.png"/>
            </bt:Images>
            <bt:Urls>
                <bt:Url id="Commands.Url" DefaultValue="https://meet-outlook-addin.cloud.site.sa/commands.html"/>
            </bt:Urls>
            <bt:ShortStrings>
                <bt:String id="Meet.sa" DefaultValue="Meet.sa"/>
                <bt:String id="ScheduleMeeting" DefaultValue="Schedule Meeting"/>
                <bt:String id="site.meet.sa.label" DefaultValue="site.meet.sa" />
                <bt:String id="site-internal.meet.sa.label" DefaultValue="site-internal.meet.sa" />
            </bt:ShortStrings>
            <bt:LongStrings>
                <bt:String id="ScheduleMeeting" DefaultValue="Schedule Meeting"/>
                <bt:String id="site.meet.sa.label" DefaultValue="site.meet.sa" />
                <bt:String id="site-internal.meet.sa.label" DefaultValue="site-internal.meet.sa" />
            </bt:LongStrings>
        </Resources>
    </VersionOverrides>
</OfficeApp>